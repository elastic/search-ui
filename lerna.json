{
  "packages": ["examples/*", "packages/*"],
  "version": "1.24.0",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "npmClient": "npm",
      "ignoreChanges": [
        "**/__tests__/**",
        "**/*.md",
        "**/src/test/**",
        "packages/search-ui-workplace-search-connector/**",
        "packages/search-ui-app-search-connector/**"
      ],
      "registry": "https://registry.npmjs.org/",
      "message": "Release: %s"
    },
    "run": {
      "sort": true,
      "noPrivate": true
    }
  }
}
