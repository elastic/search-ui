{
  "packages": [
    "examples/*",
    "packages/search-ui",
    "packages/react-search-ui",
    "packages/react-search-ui-views",
    "packages/search-ui-elasticsearch-connector",
    "packages/search-ui-engines-connector",
    "packages/search-ui-site-search-connector"
  ],
  "version": "1.24.2",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "npmClient": "npm",
      "ignoreChanges": [
        "**/__tests__/**",
        "**/*.md",
        "**/src/test/**",
        "packages/search-ui-workplace-search-connector/**",
        "packages/search-ui-app-search-connector/**",
        "packages/search-ui-analytics-plugin/**"
      ],
      "registry": "https://registry.npmjs.org/",
      "message": "Release: %s"
    },
    "run": {
      "sort": true,
      "noPrivate": true
    }
  }
}
