{
  "name": "@elastic/search-ui",
  "version": "1.12.0",
  "description": "A Headless Search UI library",
  "license": "Apache-2.0",
  "main": "lib/cjs",
  "module": "lib/esm",
  "types": "./lib/esm/index.d.ts",
  "sideEffects": false,
  "homepage": "https://docs.elastic.co/search-ui",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/search-ui.git",
    "directory": "packages/search-ui"
  },
  "scripts": {
    "test-ci": "jest --runInBand",
    "test": "jest",
    "clean": "rimraf lib",
    "watch-js": "./bin/watch-js",
    "build-js": "./bin/build-js",
    "build": "npm run clean && npm run build-js",
    "watch": "npm run clean && npm run watch-js",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/elastic/search-ui/issues"
  },
  "devDependencies": {
    "@types/history": "^4.7.11",
    "@types/qs": "^6.9.7",
    "rimraf": "^2.6.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "deep-equal": "^1.0.1",
    "history": "^4.9.0",
    "qs": "^6.7.0"
  }
}
