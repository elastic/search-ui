{
  "name": "@elastic/search-ui",
  "version": "1.6.0",
  "description": "A Headless Search UI library",
  "license": "Apache-2.0",
  "main": "lib",
  "module": "es",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/search-ui.git",
    "directory": "packages/search-ui"
  },
  "scripts": {
    "test-ci": "jest --runInBand",
    "test": "jest",
    "clean": "rimraf es lib",
    "watch-js": "./bin/watch-js",
    "build-js": "./bin/build-js",
    "build": "run-s clean build-js",
    "watch": "run-p watch-js",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/elastic/search-ui/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/runtime": "^7.5.4",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "deep-equal": "^1.0.1",
    "history": "^4.9.0",
    "qs": "^6.7.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
