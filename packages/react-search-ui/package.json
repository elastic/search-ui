{
  "name": "@elastic/react-search-ui",
  "version": "1.22.0",
  "homepage": "https://docs.elastic.co/search-ui",
  "description": "A React library for building search experiences",
  "license": "Apache-2.0",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/search-ui.git",
    "directory": "packages/react-search-ui"
  },
  "scripts": {
    "test-ci": "jest --runInBand",
    "test": "jest",
    "clean": "rimraf lib",
    "watch-js": "./bin/watch-js",
    "build-js": "./bin/build-js",
    "build": "npm run clean && npm run build-js",
    "watch": "npm run build && npm run watch-js",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/elastic/search-ui/issues"
  },
  "dependencies": {
    "@elastic/react-search-ui-views": "1.22.0",
    "@elastic/search-ui": "1.22.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 19",
    "react-dom": ">= 16.8.0 < 19"
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^2.6.3",
    "typescript": "^4.9.3"
  },
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/esm/index.d.ts"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  }
}
