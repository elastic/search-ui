{
  "name": "@elastic/search-ui-site-search-connector",
  "version": "0.2.1",
  "description": "A Search UI connector for Elastic's Site Search Service",
  "license": "Apache-2.0",
  "main": "lib",
  "module": "es",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastic/search-ui.git"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf es lib",
    "watch-js": " npm-run-all --parallel \"build-* -- --watch\"",
    "build-js": "babel src --out-dir lib --ignore '**/*.test.js'",
    "build-js-es": "BABEL_MODULES=false babel src --out-dir es --ignore '**/*.test.js'",
    "build": "npm-run-all clean build-js build-js-es",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/elastic/search-ui/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2"
  }
}
